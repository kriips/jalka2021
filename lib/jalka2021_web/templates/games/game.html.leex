<main role="main" class="container">
    <h1><%=@match.home_team.name <> " - " <>  @match.away_team.name %> <%= @match.home_score %>:<%= @match.away_score %></h1>
    <dl>
        <%= for prediction <- @predictions do %>
        <dt><%= prediction.user.name %> <%= prediction.home_score %>:<%= prediction.away_score %></dt>
        <% end %>
    </dl>
</main>
